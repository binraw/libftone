#include "libft.h"

void *ft_memcpy(void *destination, const void *source, size_t size)
{
	unsigned char *dest;
	const unsigned char *src;
	size_t i;

	dest = destination;
	src = source;
	i = 0;
	while(i < size)
	{
		dest[i] = src[i];
		i++;
	}
	return (destination);
}


