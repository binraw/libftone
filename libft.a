    return (i);
}

char *ft_strrchr(const char *string, int searchedChar)
{
    int i;
    char *p;

    p = (char *)string;
    i = ft_strlen(p);
    while (i >= 0)
    {
        if (p[i] == searchedChar)
        {

            return (&p[i]);
        }

        i--;
    }

    return NULL;
}

