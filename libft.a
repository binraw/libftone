#include "libft.h"

char *strdup(const char *source)
{
    size_t i;
    size_t y;
    char *pointer;

    
    y = 0;
    i = ft_strlen(source) + 1;
    pointer = malloc(i * sizeof(char));
    while (y < i)
    {
        pointer[y] = source[y];
        y++;
    }    
    pointer[y] = '\0';
    return (pointer);
}
