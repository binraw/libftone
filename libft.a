#include "libft.h"

char *ft_strmapi(char const *s, char (*f)(unsigned int, char))
{
    char *str;
    size_t i;
    size_t y;
    
    i = 0;
    str = ft_strdup(s);
    if (!str)
        return (NULL);
    while (str[i])
    {
        str[i] = (*f)(i , str[i]);
        i++;
    }
    return (str); 
