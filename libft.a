#include "libft.h"

char *ft_substr(char const *s, unsigned int start, size_t len)
{
    char *pointer;
    unsigned int i;
    
    pointer = malloc(len * sizeof(char));
    if(!pointer)
    {
        return (NULL);
    }
    i = 0;
    while (s[start] != '\0' && i < len)
	{
		pointer[i] = s[start];
		i++;
        start++;
	}
	pointer[i] = '\0';
    return(pointer);
    
}

