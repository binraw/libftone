#include "libft.h"
int white_space(char *str)
{
    int count;
    int i;

    count = 0;
    i = 0;
    while (str[i] != '\0' && str[i] == ' ') 
    {
        i++;
        count++;
    }
    return (count);
}
int ft_atoi(char *str)
{
    int result;
    int sign;
    int i;

    result = 0;
    sign = 1;
    i = 0 + white_space(str);
    if (str[i] == '-')
    {
        sign = -1;
    }
    else if (str[i] == '+')
    {
        i++;
    }
    while (str[i] != '\0' && str[i] >= 48 && str[i] <= 57)
    {
        result = ((result * 10) + (str[i] - 48)) * sign;
    }
    return (result);   
}
